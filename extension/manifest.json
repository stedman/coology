{
	"manifest_version": 2,
	"name": "Coology",
	"description": "Awesome Schoology themes extension",
  "version": "0.12.2",
  "icons": {
    "48": "icons/Coology-48.png",
    "96": "icons/Coology-96.png"
  },
	"content_scripts": [{
		"matches": ["*://*.schoology.com/*"],
		"css": ["coology.css", "themes.css"],
		"js": ["coology.js"],
		"run_at": "document_end"
  }],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage"
  ],
	"author": "Steve Stedman"
}
